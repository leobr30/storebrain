model Document {
  id        Int          @id @default(autoincrement())
  fileName  String
  mimeType  String
  filePath  String
  userId    Int
  user      User         @relation(fields: [userId], references: [id])
  type      DocumentType
  createdAt DateTime     @default(now())
}

enum DocumentType {
  CNI
  VITAL_CARD
  MUTUAL_CARD
  RIB
  ADDRESS_PROOF
  CRIMINAL_RECORD
  RESIDENCE_PERMIT
  SICK_LEAVE
  OTHER
}

